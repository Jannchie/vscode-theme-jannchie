{
  "publisher": "jannchie",
  "name": "theme-jannchie",
  "displayName": "Jannchie Theme",
  "type": "module",
  "version": "0.3.0",
  "description": "Jannchie theme for VS Code",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jannchie/vscode-theme"
  },
  "bugs": {
    "url": "https://github.com/jannchie/vscode-theme/issues"
  },
  "keywords": [
    "theme",
    "my-theme",
    "light",
    "dark"
  ],
  "categories": [
    "Themes"
  ],
  "icon": "icon.jpg",
  "files": [
    "themes"
  ],
  "engines": {
    "vscode": "^1.43.0"
  },
  "contributes": {
    "themes": [
      {
        "label": "Jannchie Dark Soft",
        "uiTheme": "vs-dark",
        "path": "./themes/jannchie-dark-soft.json"
      },
      {
        "label": "Jannchie Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/jannchie-dark.json"
      },
      {
        "label": "Jannchie Black",
        "uiTheme": "vs-dark",
        "path": "./themes/jannchie-black.json"
      }
    ]
  },
  "scripts": {
    "build": "esno src/index.ts",
    "dev": "esno watch src/index.ts",
    "vscode:prepublish": "npm run build",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "bumpp --all && vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@antfu/utils": "^0.7.6",
    "@jannchie/eslint-config": "^2.2.0",
    "@types/color": "^3.0.4",
    "@types/node": "^20.8.6",
    "@vscode/vsce": "^2.21.1",
    "bumpp": "^9.2.0",
    "color": "^4.2.3",
    "eslint": "^8.51.0",
    "esno": "^0.17.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "preview": true
}
